%table.w-100.table-bordered.day-pdt-table
  %caption.text-center
    = Setting.month_fecals.label
  %tbody
    %tr
      %td.p-3.text-center
        = f.label nil, Setting.month_fecals.up_std, :class => " text-center"
      %td.p-3.text-center
        = f.number_field :up_std, :class => "form-control", :disabled => true, :required => true 
        
      %td.p-3.text-center
        = f.label nil, Setting.month_fecals.end_std, :class => " text-center"
      %td.p-3.text-center
        = f.number_field :end_std, :class => "form-control", :disabled => true, :required => true 
    %tr
        
      %td.p-3.text-center
        = f.label nil, Setting.month_fecals.yoy, :class => " text-center"
      %td.p-3.text-center
        = f.number_field :yoy, :class => "form-control", :disabled => true, :required => true 
        
      %td.p-3.text-center
        = f.label nil, Setting.month_fecals.mom, :class => " text-center"
      %td.p-3.text-center
        = f.number_field :mom, :class => "form-control", :disabled => true, :required => true 
    %tr
        
      %td.p-3.text-center
        = f.label nil, Setting.month_fecals.ypdr, :class => " text-center"
      %td.p-3.text-center
        = f.number_field :ypdr, :class => "form-control", :required => true 
